{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "ES2023",
      "DOM"
    ],
    "module": "NodeNext",
    "jsxImportSource": "react",
    "target": "ESNext",
    "noImplicitAny": false,
  }
}