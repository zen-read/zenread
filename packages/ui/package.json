{
  "name": "@zenread/ui",
  "imports": {
    "#*": "./src/*"
  },
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "dev:ts": "tsc --watch",
    "dev:css": "postcss ./src/**/*.css --dir ./dist --base ./src --watch",
    "dev": "concurrently \"pnpm:dev:*\"",
    "build:ts": "tsc",
    "build:css": "postcss ./src/**/*.css --dir ./dist --base ./src",
    "build": "concurrently \"pnpm:build:*\"",
    "build:icons": "node src/Icons/build.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "Mike Noired",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/manager-api": "^8.2.7",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@storybook/theming": "^8.2.7",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@zenread/shared": "workspace:*",
    "@zenread/tailwind-config": "workspace:*",
    "@zenread/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.19",
    "camelcase": "^6.0.0",
    "clsx": "^2.1.1",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "postcss-nested": "^6.2.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prop-types": "^15.8.1",
    "rimraf": "^3.0.2",
    "storybook": "^8.2.4",
    "svgo": "^3.3.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "class-variance-authority": "^0.7.0",
    "concurrently": "^8.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "exports": {
    ".": "./src/index.ts",
    "./icons": "./src/Icons/index.ts",
    "./icons/*": "./src/Icons/*"
  }
}